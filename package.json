{
    "name": "nonbox-mobile",
    "version": "1.0.0",
    "description": "Mobile companion app for nonbox https://nonbox.co",
    "scripts": {
        "setup-ios": "ionic cordova platform add ios && cd platforms/ios/cordova && npm install ios-sim",
        "ios": "ionic cordova emulate ios --target iPhone-7 -lc"
    },
    "dependencies": {
        "cordova-android": "^6.2.3",
        "cordova-ios": "^4.5.0",
        "cordova-plugin-add-swift-support": "^1.7.0",
        "cordova-plugin-device": "^1.1.7",
        "cordova-plugin-inappbrowser": "^1.7.2",
        "cordova-plugin-ionic": "^2.0.3",
        "cordova-plugin-splashscreen": "^4.1.0",
        "cordova-plugin-statusbar": "^2.3.0",
        "cordova-plugin-whitelist": "^1.3.3",
        "cordova-sqlite-storage": "^2.1.4",
        "ionic-plugin-keyboard": "^2.2.1"
    },
    "devDependencies": {
        "@ionic/cli-plugin-cordova": "1.1.2",
        "@ionic/cli-plugin-ionic1": "1.1.2",
        "bower": "^1.3.3",
        "gulp-util": "^2.2.14",
        "shelljs": "^0.3.0",
        "gulp": "^3.5.6",
        "gulp-concat": "^2.2.0",
        "gulp-minify-css": "^0.3.0",
        "gulp-rename": "^1.2.0",
        "gulp-sass": "^2.0.4",
        "ng-cordova": "^0.1.27-alpha",
        "nonbox-client": "^1.0.2"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/nonbox/nonbox-mobile"
    },
    "keywords": [
        "nonbox-client",
        "nonbox",
        "client"
    ],
    "author": "Ricky Brown",
    "bugs": {
        "url": "https://github.com/nonbox/nonbox-mobile/issues"
    },
    "cordova": {
        "plugins": {
            "cordova-plugin-device": {},
            "cordova-plugin-splashscreen": {},
            "cordova-plugin-statusbar": {},
            "cordova-plugin-whitelist": {},
            "ionic-plugin-keyboard": {},
            "cordova-plugin-ionic": {
                "APP_ID": "0979d6ae",
                "CHANNEL_NAME": "Master",
                "UPDATE_METHOD": "background",
                "UPDATE_API": "https://api.ionicjs.com",
                "MAX_STORE": "2"
            },
            "cordova-plugin-inappbrowser": {},
            "cordova-sqlite-storage": {}
        },
        "platforms": [
            "android",
            "ios"
        ]
    }
}
